{
  "version": 2,
  "builds": [
    {
      "src": "src/index.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/.*",
      "dest": "src/index.js"
    }
  ],
  "crons": [
    {
      "path": "/refresh-token",
      "schedule": "*/2 * * * *"
    },
    {
      "path": "/cron/retry-failed",
      "schedule": "*/3 * * * *"
    }
  ]
}